use{
  std.io.file
  std.io.base
  std.collection
  risa.fpl.tokenizer
  risa.fpl.base
}
class FPL{
 this String project{
  Map;String String; properties
  FileReader reader project + "/build.properties"
  Tokenizer tokenizer reader
  while tokenizer hasNext{
    var id tokenizer nextID
  }
  tokenizer close
 }
}
main{
 if argc != 2{
   print "project directory expected"
   return 2 #1 is reserved for exceptions
 }
 FPL fpl args get 1
}